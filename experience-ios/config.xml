<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ledlab.experience-ios" version="3.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Experience</name>
    <description>
        An Experience Sampling survey application for LEDlab emotional development research.
    </description>
    <author email="ledlab.neu@gmail.com" href="http://ledlab.neu.edu">
        LEDlab
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="http://ledlab.nfshost.com/data_collector.cgi" />
    <allow-navigation href="http://ledlab.nfshost.com/data_collector.cgi"/>
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
    <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
